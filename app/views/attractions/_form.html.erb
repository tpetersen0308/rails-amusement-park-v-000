<%= form_for attraction do |f| %>
  <%= f.label :name %> <br>
  <%= f.text_field :name %> <br>
  <%= f.label :min_height, "Minumum Height (in inches)" %> <br>
  <%= f.text_field :min_height %> <br>
  <%= f.label :happiness_rating, "Happiness Rating (1-5 scale)" %> <br>
  <%= f.text_field :happiness_rating %> <br>
  <%= f.label :nausea_rating, "Nausea Rating (1-5 scale)" %> <br>
  <%= f.text_field :nausea_rating %> <br>
  <%= f.label :tickets, "Ticket Requirement" %> <br>
  <%= f.text_field :tickets %> <br>
  <%= f.submit %>
<% end %>